{
  "endOfLine": "lf",
  "importOrder": [
    "^#(.*)/",
    "^[./]"
  ],
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "jsxSingleQuote": true,
  "overrides": [
    {
      "files": [
        "*.json",
        "*.jsonc",
        ".*rc"
      ],
      "options": {
        "parser": "json-stringify"
      }
    }
  ],
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all"
}
